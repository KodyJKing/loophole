{
    "name": "loophole",
    "version": "0.0.0",
    "repository": "https://github.com/KodyJKing/loophole.git",
    "author": "Kody King <kody.j.king@gmail.com>",
    "license": "none",
    "scripts": {
        "dev": "yarn install && yarn build && yarn link geode",
        "build": "tsc && yarn buildpeg",
        "buildpeg": "pegjs -o src/vm/assemblyParser.js src/vm/assemblyParser.pegjs",
        "parcel-pack-assets": "parcel build assets/**/*",
        "watch": "npm-run-all --parallel watch:*",
        "watch:peg": "nodemon --watch src/vm -e pegjs -x yarn buildpeg",
        "watch:parcel": "parcel src/index.html",
        "test": "ava --watch --verbose"
    },
    "private": true,
    "devDependencies": {
        "ava": "^2.2.0",
        "pegjs": "^0.10.0",
        "parcel": "^2.0.0-beta.2",
        "npm-run-all": "^4.1.5",
        "@babel/core": "^7.14.8",
        "@babel/preset-typescript": "^7.14.5",
        "@types/node": "^15.12.4",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0"
    },
    "dependencies": {
        "geode": "https://github.com/KodyJKing/geode.git#1.0.0"
    }
}